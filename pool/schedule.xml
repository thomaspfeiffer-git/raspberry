<?xml version="1.0" encoding="utf-8"?>

<schedules>
    <min_on_time>5</min_on_time>
    <min_off_time>30</min_off_time>

    <schedule>
        <start>5:50</start>
        <stop>6:20</stop>
    </schedule>

    <schedule>
        <start>9:00</start>
        <stop>16:00</stop>
        <conditions>
            <humidity direction="in">
                <value>96.0</value>
                <operator>&lt;=</operator>
                <hysteresis>
                    <interval_for_measurement>3</interval_for_measurement>
                    <interval_for_retry>27</interval_for_retry>
                </hysteresis>
            </humidity>
            <temperature location="outside">
                <value>1.0</value>
                <operator>&gt;=</operator>
            </temperature>
            <humidity_difference>
                <value>5.0</value>
                <hysteresis>
                    <interval_for_measurement>3</interval_for_measurement>
                    <interval_for_retry>27</interval_for_retry>
                </hysteresis>
            </humidity_difference>
        </conditions>
    </schedule>
</schedules>

